<UserControl x:Class="WSE.StudentApp.Views.ShellView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="using:WSE.StudentApp.ViewModels">
    <Grid Padding="12">
        <StackPanel Orientation="Horizontal" Spacing="12">
            <TextBlock Text="WSE Math" FontSize="20" FontWeight="Bold"/>
            <Button Content="Courses" Click="OnCourses"/>
            <Button Content="Live Class" Click="OnLiveClass"/>
        </StackPanel>
        <Frame x:Name="ContentFrame" Margin="0,50,0,0"/>
    </Grid>
</UserControl>
